###############################################
# Docker Compose
###############################################

COMPOSE_PROJECT_NAME=<DOCKER_PROJECT_NAME_HERE>

###############################################
# Common
###############################################

TZ=Europe/Paris

###############################################
# Nuxeo
###############################################

NX_IMG=nuxeo:LTS-2019
NX_PORT=8081
NX_DEV=true
NX_CLID=<PROJECT_CLID_HERE>
NX_PACKAGES="nuxeo-web-ui nuxeo-jsf-ui nuxeo-dam"
