###############################################
# Database
###############################################

NX_DB_PORT=5434
NX_DB_TYPE=postgresql
NX_DB_NAME=<DB_NAME_HERE>
NX_DB_PWD=<PASSWORD_HERE>
NX_DB_USER=<DB_USER_HERE>
NX_DB_HOST=nx-psql
